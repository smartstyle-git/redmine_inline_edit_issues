<% if params[:back_url].present? %>
    <li>
    	<%
    	uri = URI.parse(params[:back_url])
    	if uri.query.present?
	    	args = CGI.parse(uri.query)
	    	col = args.keys.include?('c[]') ? args['c[]'] : nil
	    end
	    # 常に全チケットを表示するため、idsパラメータはnilにする
	    ids_param = nil
    	%>
        <%= context_menu_link l(:button_inline_edit), @project ? edit_multiple_project_inline_issues_path(@project, :ids => ids_param, :c => col) : edit_multiple_inline_issues_path(:ids => ids_param, :back_url => params[:back_url], :c => col), :disabled => !@can[:edit] %>
    </li>
<% end %>